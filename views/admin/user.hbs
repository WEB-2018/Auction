<div class="container">
<div class="header" style="margin-left:0px">
  <h2>Users</h2>
 </div>         
  <table class="table table-hover" style="overflow-y: scroll">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Email</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody height="100px">
    {{#each users}}
      <tr>
        <td>{{idNguoiDung}}</td>
        <td>{{hoTen}}</td>
        <td>{{diaChi}}</td>
        <td>{{email}}</td>
        <td><button>delete</button></td>
      </tr>
    {{/each}}
 
    </tbody>
  </table>
</div>
