<div class="row">
            {{#each users}}
               <div class="col-md-4">
                    <div class="user-profile box-shadow bg-shape">
                        <div class="user-profile-photo">
                            <img src="/imgs/user/{{idNguoiDung}}.jpg" alt="">
                        </div>
                        <div class="user-profile-info pt-20">
                            <h5 class="member-name"><a href="" tabindex="0">{{hoTen}}</a></h5>
                            <p class="member-esb">{{diaChi}}</p>
                            <p class="mb-20">Rating: </p>
                            <ul class="footer-social list-inline">
                                <li>
                                    <a class="facebook" href="" title="Facebook" tabindex="0"><i class="zmdi zmdi-facebook"></i></a>
                                </li>
                                <li>
                                    <a class="google-plus" href="" title="Google Plus" tabindex="0"><i class="zmdi zmdi-google-plus"></i></a>
                                </li>
                                <li>
                                    <a class="twitter" href="" title="Twitter" tabindex="0"><i class="zmdi zmdi-twitter"></i></a>
                                </li>
                                <li>
                                    <a class="rss" href="" title="RSS" tabindex="0"><i class="zmdi zmdi-rss"></i></a>
                                </li>
                            </ul>  
                            <a href="/admin/manage-users/del#{{f_ID}}">
                                <button>Delete</button>
                            </a>                                   
                        </div>
                    </div>
               </div>
            {{/each}}
              
                
           </div>